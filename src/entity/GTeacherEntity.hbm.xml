<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.GTeacherEntity" table="g_teacher" schema="growingup">
        <id name="teacherId">
            <column name="teacher_id" sql-type="varchar(16)" length="16"/>
        </id>
        <property name="teacherName">
            <column name="teacher_name" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="teacherMajor">
            <column name="teacher_major" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="teacherMobileNumber">
            <column name="teacher_mobile_number" sql-type="varchar(16)" length="16" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>